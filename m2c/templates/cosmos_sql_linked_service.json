{
    "name": "{{ credential }}",
    "type": "Microsoft.DataFactory/factories/linkedservices",
    "properties": {
        "annotations": [],
        "type": "CosmosDb",
        "typeProperties": {
            "connectionString": "AccountEndpoint=https://{{ accountname }}.documents.azure.com:443/;Database={{ dbname }};",
            "encryptedCredential": "{{ credential }}"
        }
    }
}
